            "192.168.1.18"
        ],
        "error_page": {
            "500": "/le/chemin/choisi.html",
            "404": "/chemin/random.html"
        },
        "client_max_body_size": 1000,
        "root": "directory",
        "autoindex": "on",
        "index": [
            "index.php",
            "index.html"
        ],
        "location": [
            {
                "methods": [
                    "GET",
                    "POST",
                    "DELETE"
                ],
