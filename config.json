[
    {
        "listen": "127.0.0.1:8000",
        "server_name": [
            "localhost",
            "192.168.1.18"
        ],
        "error_page": {
            "500": "error_views/500.html",
            "404": "error_views/404.html"
        },
        "client_max_body_size": 1000,
        "root": "www/",
        "autoindex": "off",
        "index": [
            "index.html",
            "index.php"
        ],
        "location": [
            {
        		"autoindex": "on",
                "methods": [
                    "DELETE"
                ],
                "fastcgi_param": {
                    "fastcgi_pass": "127.0.0.1:9000",
                    "fastcgi_index": "test.php",
                    "fastcgi_param": "SCRIPT_FILENAME  /scripts$fastcgi_script_name"
                },
				"index": [
            		"index.php",
            		"index.html"
        		],
                "path": "/directory/dir"
            }
        ]
    },
    {
        "listen": 4000,
        "server_name": [
            "localhost2",
            "www.localhost2"
        ],
        "error_page": {
